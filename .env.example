# AWS/LocalStack Configuration
AWS_DEFAULT_REGION=us-east-1
AWS_ENDPOINT_URL=http://localhost:4566
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=dagster
POSTGRES_PASSWORD=dagster
POSTGRES_DB=dagster

# SQS Configuration
TASKIQ_QUEUE_NAME=dagster-taskiq-demo-taskiq-dev.fifo
TASKIQ_DLQ_NAME=dagster-taskiq-demo-taskiq-dlq-dev.fifo

# ECS Configuration
ECS_CLUSTER_NAME=dagster-taskiq-demo-dev

# Dagster Configuration
DAGSTER_HOME=/opt/dagster/dagster_home
DAGSTER_WEBSERVER_HOST=0.0.0.0
DAGSTER_WEBSERVER_PORT=3000

# TaskIQ Configuration
TASKIQ_WORKER_CONCURRENCY=4
TASKIQ_VISIBILITY_TIMEOUT=300

# Auto Scaler Configuration
AUTOSCALER_MIN_WORKERS=2
AUTOSCALER_MAX_WORKERS=20
AUTOSCALER_SCALE_UP_THRESHOLD=5
AUTOSCALER_SCALE_DOWN_THRESHOLD=2
AUTOSCALER_COOLDOWN_SECONDS=60

# Load Simulator Configuration
LOAD_SIM_JOB_INTERVAL_SECONDS=600
LOAD_SIM_FAST_JOB_DURATION_BASE=20
LOAD_SIM_FAST_JOB_DURATION_VARIANCE=10
LOAD_SIM_SLOW_JOB_DURATION_BASE=300
LOAD_SIM_SLOW_JOB_DURATION_VARIANCE=120

# Logging Configuration
LOG_LEVEL=INFO